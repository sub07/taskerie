// Static tasks configuration.
//
// Example:
[
  {
    "label": "run",
    "command": "cargo",
    "args": ["run"],
    // "env": {
    //   "RUST_BACKTRACE": "1"
    // },
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "reveal_target": "dock",
    "hide": "never",
    "shell": "system"
  },
  {
    "label": "test",
    "command": "cargo",
    "args": ["test"]
  }
]
